{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "blinkinitial",
						"type": "DatasetReference"
					},
					"name": "blinkitsource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "blinkitoutput"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Order ID} as string,",
				"          {Customer Name} as string,",
				"          Category as string,",
				"          {Sub Category} as string,",
				"          City as string,",
				"          {Order Date} as string,",
				"          Region as string,",
				"          Sales as string,",
				"          Discount as string,",
				"          Profit as string,",
				"          State as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> blinkitsource",
				"blinkitsource aggregate(groupBy(Category),",
				"     totalSales = count(Sales)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> blinkitoutput"
			]
		}
	}
}